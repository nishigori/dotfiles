# Example of zgenrc_local
#
# vim: set ts=4 sw=4 sts=0 expandtab filetype=zsh:
#
zgen oh-my-zsh plugins/ssh-agent
zgen oh-my-zsh plugins/docker
zgen oh-my-zsh plugins/docker-compose
zgen oh-my-zsh plugins/python
zgen oh-my-zsh plugins/pip
zgen oh-my-zsh plugins/pep8
zgen oh-my-zsh plugins/virtualenv
zgen oh-my-zsh plugins/virtualenvwrapper
zgen oh-my-zsh plugins/ruby
zgen oh-my-zsh plugins/rbenv
zgen oh-my-zsh plugins/gem
zgen oh-my-zsh plugins/rake
zgen oh-my-zsh plugins/bundler
zgen oh-my-zsh plugins/node
zgen oh-my-zsh plugins/npm
zgen oh-my-zsh plugins/composer
zgen oh-my-zsh plugins/vagrant
zgen oh-my-zsh plugins/aws

# NOTE: If you're static on OS, without-able case stmt.
case ${OSTYPE} in
    darwin*)
        zgen oh-my-zsh plugins/osx
        zgen oh-my-zsh plugins/brew
        zgen oh-my-zsh plugins/brew-cask
        zgen oh-my-zsh plugins/tmux
        ;;
    freebsd*)
        ;;
    linux*)
        zgen oh-my-zsh plugins/gnu-utils
        if [ -f /etc/redhat-release ]; then
            zgen oh-my-zsh plugins/yum
        elif [ -f /etc/debian_version ]; then
            zgen oh-my-zsh plugins/debian
        fi
        ;;
esac


#zgen load /path/to/super-secret-private-plugin
zgen load mitchellh/packer contrib/zsh-completions

# theme candidates: bira rkj-repos ys fox fino murilasso
zgen oh-my-zsh themes/bira
